{
  "name": "data-generator-app",
  "version": "1.0.0",
  "description": "Data Generator - Full Stack Databricks App",
  "scripts": {
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install:frontend": "cd frontend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python app.py",
    "start": "concurrently --names 'API,WEB' --prefix-colors 'blue,green' 'npm run dev:backend' 'npm run dev:frontend'",
    "build": "npm run build:frontend",
    "setup": "npm run install:backend && npm run install:frontend"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": ["databricks", "sql", "generator", "react", "flask", "python"],
  "author": "Data Generator Team",
  "license": "MIT"
}
