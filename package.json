{
  "name": "data-generator-ts",
  "version": "1.0.0",
  "description": "TypeScript/Node.js version of the Data Generator for Databricks",
  "main": "dist/cli.js",
  "bin": {
    "data-generator-ts": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsx watch src/cli.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "keywords": ["databricks", "sql", "generator", "typescript", "yaml"],
  "author": "Data Generator Team",
  "license": "MIT",
  "dependencies": {
    "yaml": "^2.3.4",
    "@faker-js/faker": "^8.4.1",
    "moment": "^2.29.4",
    "fs-extra": "^11.2.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
} 